{#
/**
 * @file
 * Template for a block.
 */
#}
{% embed '@radix/block/block.twig' with {
  html_tag: 'div',
  utility_classes: [
    'container',
  ]
} %}
  {% block content %}
    <div class="row">
      <div class="col-md-6">
        {{ content }}
      </div>
    </div>
  {% endblock %}
{% endembed %}
