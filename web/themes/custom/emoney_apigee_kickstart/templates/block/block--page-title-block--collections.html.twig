{#
/**
 * @file
 * Template for the page title block.
 */
#}
{% embed '@radix/block/block.twig' with {
  html_tag: 'div'
} %}
  {% block content %}
    <div class="container">
      <div class="row">
        <div class="col-md-10 offset-md-1 p-0">
          <h1{{ title_attributes.addClass('page__title', 'mb-0') }}>{{ content }}</h1>
          <div{{ title_attributes.addClass('page__headline') }}>APIs that meet your specific needs.</div>
        </div>
      </div>
    </div>
  {% endblock %}
{% endembed %}
